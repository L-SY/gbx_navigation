<launch>
    <!-- Run the map server -->
    <node name="map_server" pkg="map_server" type="map_server" args="$(find navigation_config)/map/E3_121.yaml"/>

    <node pkg="tf" type="static_transform_publisher" name="map_to_odom"
          args="0 0 0 0 0 0 odom map 100" />

    <include file="$(find fast_lio)/launch/mapping_mid360.launch"/>
    <include file="$(find fast_lio_localization_qn)/launch/run_loc_qn.launch"/>

    <!--- Run Move_Base -->
    <node pkg="move_base" type="move_base" respawn="false" name="move_base"  output="screen">
        <rosparam file="$(find navigation_config)/config/teb_planner_params/teb_planner_params.yaml" command="load"/>
<!--        <rosparam file="$(find navigation_config)/config/teb_planner_params/teb_planner_params_carlike.yaml" command="load"/>-->
        <rosparam file="$(find navigation_config)/config/teb_planner_params/move_base.yaml" command="load"/>
        <rosparam file="$(find navigation_config)/config/teb_planner_params/costmap_common.yaml" command="load" ns="global_costmap"/>
        <rosparam file="$(find navigation_config)/config/teb_planner_params/costmap_common.yaml" command="load" ns="local_costmap"/>
        <rosparam file="$(find navigation_config)/config/teb_planner_params/costmap_global.yaml" command="load" ns="global_costmap"/>
        <rosparam file="$(find navigation_config)/config/teb_planner_params/costmap_local.yaml" command="load" ns="local_costmap"/>
    </node>


</launch>